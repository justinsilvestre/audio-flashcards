      "flashcard": {
        "id": "f9b7895a-7542-4d7b-a830-9a505f7131a6",
        "fields": {
          "front": "Nichts leichter als das! antwortete Frederick.",
          "back": "Piece of cake (lit. nothing easier than that)! replied Frederick."
        }
      }
    },
