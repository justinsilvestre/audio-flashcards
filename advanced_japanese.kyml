    image:
      type: VideoStill
  - id: f39634cd-aad6-4636-ab3b-de78d5aa0dc3
    start: 15:07.13
    end: 15:09.22
    fields:
      transcription: 手緩いな　ジェダイト
      meaning: You are too lenient, Jadeite.
    tags: *a1
    image:
      type: VideoStill
  - id: 41753a0f-e628-4a89-a09b-8b43f383d313
