      "end": 897,
      "flashcard": {
        "id": "c4a94f27-fb0d-4298-9da3-37bbe65fc69c",
        "fields": {
          "front": "«Frederick», fragte Piggeldy seinen großen Bruder, «zeig mir, wie man den Mond fangen kann.»",
          "back": "Frederick, Piggeldy asked his older brother, show me how one can catch the moon."
        }
      }
    },
    "f9b7895a-7542-4d7b-a830-9a505f7131a6": {
