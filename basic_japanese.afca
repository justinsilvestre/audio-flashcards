    "acf0f98a-b076-42a8-8d24-d38ee3f2a405": {
      "start": 21333,
      "end": 21649,
      "id": "acf0f98a-b076-42a8-8d24-d38ee3f2a405",
      "fileId": "b0cd359c-28f9-424e-a643-21cd368d4e19",
      "flashcard": {
        "id": "acf0f98a-b076-42a8-8d24-d38ee3f2a405",
        "tags": [
          "shad1-7"
        ],
        "fields": {
          "front": "",
          "back": "Yamada-san, shúmi-wa nán-desu ka?\nShúmi? Uun, shashin ka na.",
          "717524da-c2af-4196-b850-a6ab9a646798": "What are your hobbies, Yamada-san?\nHobbies? Hm, photography?",
          "4bb9ac55-5d96-4561-b6b2-c9827c277d16": "\"ka na\" gives the sentence the tone of, \"I wonder...?\""
        }
      }
    },
