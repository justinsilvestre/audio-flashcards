    image:
      type: VideoStill
  - id: 800f9d8f-6856-4fa4-aa0a-9dedd95bf6c4
    start: 16:29.44
    end: 16:33.73
    fields:
      transcription: 早く来い　プリンセス　{{c1::さもないと}}
      meaning: Hurry up, Princess. Or else...
    tags: *a2
    image:
      type: VideoStill
  - id: 692af8be-ab00-4c35-b2cb-fda308a1f0b3
