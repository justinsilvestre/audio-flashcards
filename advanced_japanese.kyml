    tags: *a1
    image:
      type: VideoStill
  - id: 9cfb1978-a637-4061-b6a8-abc90327c3c3
    start: 15:58.94
    end: 16:01.36
    fields:
      transcription: もうちょっと{{c1::出力}}が大きかったら
