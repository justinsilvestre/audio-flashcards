    "bf103e02-3c8e-413d-a2a2-a01cd660413e": {
      "id": "bf103e02-3c8e-413d-a2a2-a01cd660413e",
      "fileId": "b0cd359c-28f9-424e-a643-21cd368d4e19",
      "start": 18301,
      "end": 18496,
      "flashcard": {
        "id": "bf103e02-3c8e-413d-a2a2-a01cd660413e",
        "tags": [
          "shad1-6"
        ],
        "type": "Transliteration",
        "fields": {
          "transcription": "今、何時？\nえーっと、４時２０分。",
          "pronunciation": "Íma, nánji?\nEetto, yó-ji ni-júp-pun.",
          "meaning": "What time is it n",
          "notes": "Another instance of two-mora (two-\"syllable\") numbers getting their second syllable assimilated into a measure word. Plus, the measure word gets transformed as well in some cases.\n\ne.g.\nichí + fun = íp-pun (one minute)\nsán + fun = sán-pun (three minutes)\nrokú + fun = róp-pun (six minutes)\nhachí + fun = háp-ppun (eight minutes)\njúu + fun = júp-pun (ten minutes)\n"
        }
      }
    },
